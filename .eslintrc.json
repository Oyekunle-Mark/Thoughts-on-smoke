{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "arrow-parens": 0,
    "no-use-before-define": 0
  }
}
